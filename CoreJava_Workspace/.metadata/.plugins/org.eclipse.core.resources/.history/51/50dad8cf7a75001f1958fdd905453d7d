import com.webobjects.appserver.WOComponent;
import com.webobjects.appserver.WOContext;
import com.webobjects.foundation.NSMutableArray;

public class Main extends WOComponent {
	private static final long serialVersionUID = 1L;
	public String empname;
	public String address;
	public boolean save;
	protected boolean isMale;
	protected boolean isFemale;
	protected boolean isEnglish;
	protected boolean isHindi;
	protected boolean isTelugu;
	protected String gender;
	
	 String kl;
	NSMutableArray Knownlanguage=new NSMutableArray();
	public Main(WOContext context) {
		super(context);
		
	}
	public WOComponent submit() {
		save=true;
		System.out.println(empname);
		System.out.println(address);
		System.out.println(gender);
		System.out.println(Knownlanguage);
		if(isMale) {
			gender="Male";
			
		}
		else if(isFemale) {
			gender ="Female";
		}
		else if(isEnglish){
			Knownlanguage.addObject("English");
		}
		else if(isHindi) {
			Knownlanguage.addObject("Hindi");
		}
		else if(isTelugu) {
			Knownlanguage.addObject("Telugu");
		}
		
		return null;
	}
	
	public WOComponent back() {
		 save=false;
		empname="";
		address="";
		gender="";
		kl="";
		return null;
	}
}
